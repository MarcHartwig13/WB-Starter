{# SVG ICONS#}
{# Used these for more complex animations #}

{# Indicate that a user can get to further content #}
{% macro arrowIcon(direction = 'up') %}{% spaceless %}
    {% if direction == 'up' or direction == 'down' %}
        <svg data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 27.39 16.49"><title>arrow_up</title><path fill="#ef0d1b" d="M27.39 14.86l-1.75 1.63L13.7 3.65 1.75 16.49 0 14.86 13.7.13l13.69 14.73z"></path></svg>
    {% else %}
        <svg data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16.36 27.39"><title>arrow_right</title><path fill="#ef0d1b" d="M1.63 27.39L0 25.64 12.84 13.7 0 1.75 1.63 0l14.73 13.7L1.63 27.39z"></path></svg>
    {% endif %}
{% endspaceless %}{% endmacro %}

{% macro arrow(config) %}
    {% set color = config.color ?? '' %}
    {% set direction = config.direction ?? 'up' %}

    <div class="c_arrow c_arrow--{{ color }} c_arrow--{{ direction }}"><span class="icon">{{ _self.arrowIcon(direction) }}</span><span class="line"></span></div>
{% endmacro %}

{# Toggles the main menu on and off #}
{% macro hamburger() %}<svg id="hamburger" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 30.82 27.39"><title>hamburger</title> <rect id="top" x="0.09" width="30.73" height="3.63" fill="#fff"></rect> <rect id="middle" x="0.09" y="11.88" width="30.73" height="3.63" fill="#fff"></rect> <rect id="bottom" x="0.09" y="23.76" width="30.73" height="3.63" fill="#fff"></rect></svg>{% endmacro %}

