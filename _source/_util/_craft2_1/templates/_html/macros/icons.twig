{% macro socialLinks(config = { links: {} }) %}{% spaceless %}
    {% set modifiers = config.modifiers ?? '' %}

    <nav class="c_social_links {{ modifiers }}">
        {% if config.links.facebook ?? false %}<a class="facebook" href="{{ url(config.links.facebook) }}">{{ _self.socialIcon('facebook') }}</a>{% endif %}
        {% if config.links.twitter ?? false %}<a class="twitter" href="{{ url(config.links.twitter) }}">{{ _self.socialIcon('twitter') }}</a>{% endif %}
        {% if config.links.linkedin ?? false %}<a class="linkedin" href="{{ url(config.links.linkedin) }}">{{ _self.socialIcon('linkedin') }}</a>{% endif %}
        {% if config.links.youtube ?? false %}<a class="youtube" href="{{ url(config.links.youtube) }}">{{ _self.socialIcon('youtube') }}</a>{% endif %}
        {% if config.links.pinterest ?? false %}<a class="pinterest" href="{{ url(config.links.pinterest) }}">{{ _self.socialIcon('pinterest') }}</a>{% endif %}
        {% if config.links.instagram ?? false %}<a class="instagram" href="{{ url(config.links.instagram) }}">{{ _self.socialIcon('instagram') }}</a>{% endif %}
        {% if config.links.snapchat ?? false %}<a class="snapchat" href="{{ url(config.links.snapchat) }}">{{ _self.socialIcon('snapchat') }}</a>{% endif %}
        {% if config.links.email ?? false %}<a class="email" href="{{ 'mailto:' not in config.links.email ? 'mailto:' : '' }}{{ config.links.email }}">{{ _self.socialIcon('email') }}</a>{% endif %}
    </nav>
{% endspaceless %}{% endmacro %}

{% macro socialShare(config = {}) %}
    {% set image = config.image ?? null %}
    {% set modifiers = config.modifiers ?? '' %}
    {% set title = config.title ?? '' %}
    {% set url = config.url ?? '#' %}

    {% set links = {
        'facebook': 'https://www.facebook.com/sharer.php?app_id=828593323821891&display=popup&u=' ~ url,
        'twitter': 'https://twitter.com/intent/tweet?url=' ~ url ~ '&related=dixonschwabl&text=' ~ title,
        'linkedin': 'http://www.linkedin.com/shareArticle?mini=true&amp;url=' ~ url ~ '&amp;title=' ~ title,
        'email': 'mailto:?subject=' ~ title ~ '&body=' ~ url,
    } %}
    {% if image ?? false %}
        {% set links = links|merge({ 'pinterest': 'http://pinterest.com/pin/create/button/?url=' ~ url(url) ~ '&media=' ~ image ~ '&description=' ~ title, }) %}
    {% endif %}
    {% set config = config|merge({ 'links': links, 'modifiers': modifiers, 'title': title }) %}

    {{ _self.socialLinks(config) }}
{% endmacro %}