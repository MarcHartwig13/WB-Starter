{# @var craft \craft\web\twig\variables\CraftVariable #}
{# @var entry \craft\elements\Entry #}

<!DOCTYPE html>

{% minify %}
  <html class="no-js" lang="en">
    <head>
<%- include('ejs_includes/_head_meta') %>
      {{ adminbar() }}

<%- include('ejs_includes/_head_scripts') %>

<%- include('ejs_includes/_head_tracking') %>
    </head>
    <body>
      {% minify %}
      <div id="page">
        {% block header %}
          {% include 'partials/_ui' %}
        {% endblock %}

        <section class="page_wrapper">
          {% block contentBefore %}{% endblock %}
          {% block content %}{% endblock %}
          {% block contentAfter %}{% endblock %}
        </section>

        {% block footer %}
          {% include 'partials/_footer' %}
        {% endblock %}
      </div>
      {% endminify %}

      {% block jsFooter %}
        <script>loadjs('{{ siteUrl }}js/app.js?v=' + jsVersion);</script>
      {% endblock %}
    </body>
  </html>
{% endminify %}