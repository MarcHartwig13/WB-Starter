{# @var craft \craft\web\twig\variables\CraftVariable #}

{% macro default(options) %}{% spaceless %}
    {% set label = options.label ?? '' %}
    {% set classes = options.classes ?? '' %}
    {% set target = options.target ?? null %}
    {% set url = options.url ?? '#' %}

    <a class="c_button{% if classes ?? false %} {{ classes }}{% endif %}" href="{{ url(url) }}"{% if target ?? false %} target="{{ target }}"{% endif %}>{{ label }}</a>
{% endspaceless %}{% endmacro %}