<% if (enable_system_js === 'true') { %>
    <script>
        if(/MSIE \d|Trident.*rv:/.test(navigator.userAgent)) {
            document.write('\x3Cscript src="<%= site_root %>js/_lib/es6-promise.auto.js?v=<%= version %>">\x3C/script>');
        }
    </script>
    <script src='<%= site_root %>js/_lib/system.js?v=<%= version %>'></script>
    <script>
        SystemJS.config({
            baseURL: jsThemePath+'js/',
            paths: jsModulePaths,
            meta: {},
            transpiler: false,
        });
    </script><% } %>