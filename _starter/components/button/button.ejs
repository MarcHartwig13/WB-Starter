<% const attributes = c.attributes || '',
         classes = (c.classes || false) ? ` ${ c.classes }` : ''
         label = c.label || '',
         target = c.target || false,
         url = c.url || false;
%>

<<% if (url) { %>a href="<%- url %>"<% if (target) { %> target="<%- target %>"<% } %><% } else { %>button<% } %> class="c_button<%- classes %>" <%- attributes %>>
    <%- label %>
</<% if (url) { %>a<% } else { %>button<% } %>>